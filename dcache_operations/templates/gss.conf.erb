######################################################################
##
## DO NOT EDIT
## generated by puppet module :  <%= @name %>
## node                       :  <%= @fqdn %>
##
######################################################################
<% @gss_layout.each do |plugin| -%>
<% if plugin == "com.sun.security.jgss.krb5.accept" -%>
com.sun.security.jgss.krb5.accept {
      com.sun.security.auth.module.Krb5LoginModule required
      keyTab="/etc/krb5.keytab"
      useKeyTab=true
      storeKey=true
      debug=false
      doNotPrompt=true
      isInitiator=false
      principal="host/<%= @fqdn %>@DESY.DE";
      };
<% end -%>
<% if plugin == "Krb5Gplazma" -%>
Krb5Gplazma {
    com.sun.security.auth.module.Krb5LoginModule required debug=false useTicketCache=false;
};
<% end -%>
<% if plugin == "LdapGplazma" -%>
LdapGplazma {
   com.sun.security.auth.module.LdapLoginModule REQUIRED
             userProvider="ldap://it-ldap-slave/ou=people,ou=RGY,o=DESY,c=DE"
             userFilter="(uid={USERNAME})"
             useSSL=false;
};
<% end -%>
<% end %>
